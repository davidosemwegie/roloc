{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components": [
                "./src/components/index"
            ],
            "@layouts": [
                "./src/layouts/index"
            ],
            "@stores": [
                "./src/stores/index"
            ],
            "@utils": [
                "./src/utils/index"
            ],
            "@screens/*": [
                "./src/screens/*"
            ],
            "@types": [
                "./src/types/index"
            ],
            "@assets/*": [
                "./src/assets/*"
            ],
            "local-storage": [
                "./src/utils/local-storage"
            ],
            "@fb": [
                "./firebaseConfig"
            ],
        }
    },
    "extends": "expo/tsconfig.base"
}